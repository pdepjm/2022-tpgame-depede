import wollok.game.*
import personaje.*
import zombie.*

program juego {
	game.width(30)
 	game.height(19)
 	game.cellSize(50)
 	game.addVisualCharacter(personaje)
 	game.addVisual(zombie)
    game.onTick(600, "movimiento en el eje x", { zombie.moverseX() })
    game.onTick(600,"Movimiento en el eje y", {zombie.moverseY()})
    game.boardGround("doom.jpg")
 	
    game.start()
    
  }
