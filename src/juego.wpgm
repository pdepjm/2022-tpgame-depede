import wollok.game.*
import personaje.*
import instrucciones.*

program juego {
    const music = game.sound("doomMusic.mp3")
	var listaZombies = []
	instrucciones.crearEspacio()
	instrucciones.agregarPersonajes(listaZombies, personaje)
 	personaje.inicializarTeclas()
	instrucciones.musica(music)
    game.start()
}